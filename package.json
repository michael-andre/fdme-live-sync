{
  "name": "fdme-live-sync",
  "version": "0.1.5",
  "description": "A live reporting background app for FDME",
  "main": "dist/main.js",
  "author": "Michaël André",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint --ext .ts ./src",
    "start": "npm run build && electron . --mode=test",
    "pack": "electron-builder --dir --win",
    "dist": "electron-builder --win"
  },
  "dependencies": {
    "electron-log": "^5.0.3",
    "firebase": "^10.7.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "rxjs": "^7.8.1",
    "tesseract.js": "^5.0.4",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.4.1",
    "@types/w3c-image-capture": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "electron": "^27.2.3",
    "electron-builder": "^24.9.1",
    "firebase": "^10.7.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "rxjs": "^7.8.1",
    "tesseract.js": "^5.0.4",
    "typescript": "^5.3.3"
  },
  "build": {
    "appId": "fdme-live-sync",
    "extends": null,
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "portable"
    }
  }
}
