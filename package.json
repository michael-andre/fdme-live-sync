{
  "name": "fdme-live-sync",
  "version": "0.1.1",
  "description": "A live reporting background app for FDME",
  "main": "dist/main.js",
  "author": "Michaël André",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint --ext .ts ./src",
    "start": "npm run build && electron .",
    "pack": "electron-builder --dir --win",
    "dist": "electron-builder --win"
  },
  "dependencies": {
    "firebase": "^10.5.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "rxjs": "^7.8.1",
    "tesseract.js": "^4.1.4",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.10",
    "@types/luxon": "^3.3.3",
    "@types/w3c-image-capture": "^1.0.9",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "electron": "^27.0.1",
    "electron-builder": "^24.6.4",
    "firebase": "^10.5.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "rxjs": "^7.8.1",
    "tesseract.js": "^5.0.2",
    "typescript": "^5.2.2"
  },
  "build": {
    "appId": "fdme-live-sync",
    "extends": null,
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "portable"
    }
  }
}
