{
  "name": "fdme-live-sync",
  "version": "1.0.0",
  "description": "A live reporting background app for FDME",
  "main": "dist/main.js",
  "author": "Michaël André",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint --ext .ts ./src",
    "start": "npm run build && electron .",
    "pack": "electron-builder --dir --win",
    "dist": "electron-builder --win"
  },
  "dependencies": {
    "firebase": "^9.21.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "rxjs": "7.8.1",
    "tesseract.js": "^4.0.6",
    "tslib": "^2.5.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/luxon": "^3.3.0",
    "@types/w3c-image-capture": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "electron": "^24.2.0",
    "firebase": "^9.21.0",
    "lodash-es": "^4.17.21",
    "luxon": "^3.3.0",
    "rxjs": "^7.8.1",
    "tesseract.js": "^4.0.6",
    "typescript": "^5.0.4"
  },
  "build": {
    "appId": "fdme-live-sync",
    "extends": null,
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "portable"
    }
  }
}
